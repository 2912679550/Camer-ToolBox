<launch>

    <node pkg="imu_utils" type="imu_an" name="imu_front_cali" output="screen">
    	<!--TOPIC名称和上面一致-->
        <param name="imu_topic" type="string" value= "/imu_front_topic"/>
        <!--imu_name 无所谓-->
        <param name="imu_name" type="string" value= "front_imu"/>
        <!--标定结果存放路径-->
        <param name="data_save_path" type="string" value= "$(find imu_utils)/data/"/>
        <!--数据录制时间-min-->
        <param name="max_time_min" type="int" value= "65"/>
        <!--采样频率，即是IMU频率，采样频率可以使用rostopic hz /camera/imu查看，设置为200，为后面的rosbag play播放频率-->
        <param name="max_cluster" type="int" value= "100"/>
    </node>
    
    <node pkg="imu_utils" type="imu_an" name="imu_back_cali" output="screen">
    	<!--TOPIC名称和上面一致-->
        <param name="imu_topic" type="string" value= "/imu_back_topic"/>
        <!--imu_name 无所谓-->
        <param name="imu_name" type="string" value= "back_imu"/>
        <!--标定结果存放路径-->
        <param name="data_save_path" type="string" value= "$(find imu_utils)/data/"/>
        <!--数据录制时间-min-->
        <param name="max_time_min" type="int" value= "65"/>
        <!--采样频率，即是IMU频率，采样频率可以使用rostopic hz /camera/imu查看，设置为200，为后面的rosbag play播放频率-->
        <param name="max_cluster" type="int" value= "100"/>
    </node>

</launch>
